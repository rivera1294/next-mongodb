{
  "name": "notes-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy:prod:build-send-restart": "deploy-app -d prod:build-send-next && deploy-app -d prod:copy-server-dist",
    "lint-staged": "lint-staged",
    "git:pull": "bash ./bin/pull.sh",
    "make-js-server": "bash ./bin/transpile-server.sh && yarn transpile-socket-logic",
    "transpile-socket-logic": "node_modules/.bin/tsc --project ./tsconfig.socket-logic.json --downlevelIteration",
    "dev": "yarn make-js-server && PORT=9000 NODE_ENV=development node server-dist/run.js",
    "build": "yarn make-js-server && NODE_ENV=production next build",
    "start:prod": "yarn make-js-server && PORT=9000 NODE_ENV=production node server-dist/run.js",
    "restart": "fuser -k -n tcp 9000 && PORT=9000 NODE_ENV=production node ./server-dist/run.js",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "lint": "node_modules/.bin/eslint . --ext ts --ext tsx --ext js",
    "lint:fix": "eslint --fix",
    "format": "node_modules/.bin/prettier --write \"**/*.{js,jsx,ts,tsx}\" !.next !node_modules",
    "postinstall": "husky install .config/husky",
    "deploy-app:init": "node_modules/.bin/deploy-app init",
    "test": "echo test"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "yarn format",
      "yarn lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node ./scripts/write-task-name-to-commit",
      "pre-push": "yarn test"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "add": "^2.0.6",
    "animate.css": "^4.1.1",
    "axios": "^0.21.0",
    "axios-retry": "^3.1.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "build-url": "^6.0.1",
    "clsx": "^1.1.1",
    "core-js": "2",
    "deploy-app": "^1.0.3",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "express-graphql": "^0.12.0",
    "express-validator": "^6.7.0",
    "graphql": "^15.4.0",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "markdown-it": "^12.0.2",
    "mongoose": "^5.9.6",
    "next": "^9.3.6",
    "nextjs-progressbar": "^0.0.7",
    "react": "16.13.1",
    "react-cookie": "^4.0.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "16.13.1",
    "react-emoji-render": "^1.2.4",
    "react-headroom": "^3.1.0",
    "react-jsx-parser": "^1.28.1",
    "react-markdown": "^5.0.3",
    "react-markdown-editor-lite": "^1.2.2",
    "react-notifications-component": "^3.0.3",
    "react-syntax-highlighter": "^15.3.1",
    "react-use": "^15.3.4",
    "react-youtube": "^7.13.0",
    "remark-gfm": "^1.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "slugify": "^1.4.6",
    "socket.io": "^3.0.4",
    "socket.io-client": "^3.0.4",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@types/react-custom-scrollbars": "^4.0.7",
    "@types/react-notifications-component": "^2.4.0",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/socket.io": "^2.1.12",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react-optimize": "^1.0.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^5.0.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
